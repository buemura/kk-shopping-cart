{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsup src/main.ts --format cjs",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/main.ts --format cjs --watch --onSuccess \"node dist/main.js\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "node dist/main.js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --detectOpenHandles --watchAll"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "logger": "workspace:*",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.12",
    "eslint": "^7.32.0",
    "eslint-config-custom-server": "workspace:*",
    "jest": "^26.6.3",
    "jest-presets": "workspace:*",
    "supertest": "^6.2.4",
    "rimraf": "^4.4.1",
    "tsconfig": "workspace:*",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  }
}
