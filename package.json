{
  "name": "shopping-cart",
  "version": "1.0.0",
  "description": "The shopping cart main repository.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buemura/shopping-cart.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/buemura/shopping-cart#readme",
  "packageManager": "pnpm@7.15.0",
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean",
    "start:dev": "turbo start:dev --no-cache  --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "test": "turbo test",
    "test:watch": "turbo test -- --watchAll",
    "prepare": "husky install",
    "docker:up": "docker-compose build && docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "husky": "^8.0.3",
    "prettier": "^2.7.1",
    "turbo": "^1.9.9"
  }
}
